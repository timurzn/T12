global


defaults
        log global
        mode    http
        timeout connect 5000
        timeout client 5000
        timeout server 5000


frontend http_front
        bind *:80
        stats uri /haproxy?stats
        default_backend http_back


backend http_back
        balance roundrobin
        mode http
        server web1 {{ web1 }}:80 check
        server web2 {{ web2 }}:80 check



